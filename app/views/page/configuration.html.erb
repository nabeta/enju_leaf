<div class="column" id="col1">
  <div class="portlet" id="library">
    <h2 class="title"><%= t('activerecord.models.library') -%></h2>
    <div class="config_box">
      <ul>
        <li><%= link_to t('activerecord.models.library_group'), library_groups_path -%></li>
        <li><%= link_to t('activerecord.models.library'), libraries_path -%>
          <ul>
            <li><%= link_to t('activerecord.models.shelf'), shelves_path -%></li>
          </ul>
        </li>
        <li><%= link_to t('activerecord.models.user_group'), user_groups_path -%></li>
        <% if defined?(EnjuCirculation) %>
          <li><%= link_to t('activerecord.models.checkout_type'), checkout_types_path -%>
            <ul>
              <li><%= link_to t('activerecord.models.user_group_has_checkout_type'), user_group_has_checkout_types_path -%></li>
              <li><%= link_to t('activerecord.models.carrier_type_has_checkout_type'), carrier_type_has_checkout_types_path -%></li>
            </ul>
          </li>
        <% end %>
        <% if defined?(EnjuEvent) %>
          <li><%= link_to t('activerecord.models.event_category'), event_categories_path -%>
        <% end %>
        <ul>
          <li><%= link_to t('page.new', :model => t('activerecord.models.event_import_file')), new_event_import_file_path %></li>
        </ul>
        </li>
        <li><%= link_to t('activerecord.models.bookstore'), bookstores_path -%></li>
        <% if defined?(EnjuMessage) %>
          <li><%= link_to t('activerecord.models.message_template'), message_templates_path -%></li>
        <% end %>
        <li><%= link_to t('activerecord.models.search_engine'), search_engines_path -%></li>
        <% if defined?(EnjuBarcode) %>
          <li><%= link_to t('activerecord.models.barcode'), barcodes_path -%></li>
        <% end %>
      </ul>
    </div>
  </div>
  <div class="portlet" id="resource">
    <h2 class="title"><%= t('page.resource') -%></h2>
    <div class="config_box">
      <ul>
        <li><%= link_to t('activerecord.models.manifestation'), manifestations_path -%>
        <ul>
          <li><%= link_to t('item.all_item'), items_path %></li>
        </ul>
        </li>
        <% if defined?(EnjuBookmark) %>
          <li><%= link_to t('activerecord.models.bookmark'), bookmarks_path -%></li>
        <% end %>
        <li><%= link_to t('activerecord.models.import_request'), import_requests_path -%></li>
        <li><%= link_to t('activerecord.models.series_statement'), series_statements_path -%></li>
        <li><%= link_to t('activerecord.models.picture_file'), picture_files_path -%></li>
        <% if defined?(EnjuPurchaseRequest) %>
          <li><%= link_to t('activerecord.models.purchase_request'), purchase_requests_path -%></li>
          <li><%= link_to t('activerecord.models.order_list'), order_lists_path -%></li>
        <% end %>
        <li><%= link_to t('activerecord.models.subscription'), subscriptions_path -%></li>
        <!-- <li><%= link_to t('activerecord.models.donate'), donates_path -%></li> -->
        <% if defined?(EnjuInventory) %>
          <li><%= link_to t('activerecord.models.inventory'), inventory_files_path -%></li>
        <% end %>
        <% if defined?(EnjuResourceMerge) %>
          <li><%= link_to t('activerecord.models.patron_merge_list'), patron_merge_lists_path -%></li>
        <% end %>
      </ul>
    </div>
  </div>
</div>

